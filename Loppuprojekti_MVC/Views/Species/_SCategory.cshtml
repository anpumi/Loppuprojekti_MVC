@model IEnumerable<Loppuprojekti_MVC.Models.SpeciesCategory>

@using Loppuprojekti_MVC.Models;


@{
    ViewData["Title"] = "SCategory";
}

@*<h1>List of all threatened species in category @ViewBag.C</h1>*@
@foreach (var s in Model)
{
    @*<ul><i>@s.ScientificName</i>, classified as @s.Category</ul>*@

    <ul>
        <i> @Html.ActionLink(s.ScientificName, "SingleSpecies", "Species", new { searchTerms = s.ScientificName }),</i>
    </ul>
}